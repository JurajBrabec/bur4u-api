{
  "name": "bur4u-api",
  "version": "1.0.0",
  "description": "BUR 4U API",
  "main": "./src/bur4u-api.js",
  "scripts": {
    "build": "esbuild ./src/bur4u-api.js --bundle --minify --platform=node --outfile=./dist/bur4u-api.js",
    "build-modules": "esbuild ./src/modules.js --bundle --minify --platform=node --outfile=./dist/modules.js",
    "dev-api": "nodemon --watch src ./src/bur4u-api.js --module api",
    "dev-proxy": "nodemon --watch src --delay 2 ./src/bur4u-api.js --module proxy --port 443",
    "prod-api": "node ./dist/bur4u-api.js --module api",
    "prod-proxy": "node --delay 2 ./dist/bur4u-api.js --module proxy --port 443",
    "test": "jest"
  },
  "author": "Juraj Brabec",
  "homepage": "https://github.com/JurajBrabec/bur4u-api",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.13.4",
    "jest": "^27.0.6",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "apicache-plus": "^2.2.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "njwt": "^1.1.0",
    "node-fetch": "^2.6.1",
    "rotating-file-stream": "^2.1.5"
  }
}
